	<ThingDef ParentName="CavePlantCrops">
		<defName>Biomes_PaleBeets</defName>
		<label>pale beets</label>
		<description>A pale purple beet that has been genetically engineered to be able to burrow its roots through stone and survive off what little nutrients it can get in the caverns.</description>
		<thingClass>Terra.PlantPlus</thingClass>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/Crops/PaleBeets</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>10</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0</fertilityMin>
			<fertilitySensitivity>0.5</fertilitySensitivity>
			<growDays>3</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestYield>7</harvestYield>
			<harvestTag>Standard</harvestTag>
			<neverBlightable>true</neverBlightable>
			<harvestedThingDef>Biomes_RawPaleBeets</harvestedThingDef>
			<harvestMinGrowth>0.65</harvestMinGrowth>
			<harvestWork>40</harvestWork>
			<sowWork>110</sowWork>
			<sowMinSkill>5</sowMinSkill>
			<sowTags>
				<li>Ground</li>
				<li>RaisedBed</li>
			</sowTags>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.0</max>
			</visualSizeRange>
			<topWindExposure>0.05</topWindExposure>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<purpose>Food</purpose>
		</plant>
		<modExtensions>
			<li Class="Terra.ModExt_Plant">
				<plantSize>Small</plantSize>
				<fertilityConsumptionPerDay>0</fertilityConsumptionPerDay>
				<moistureConsumptionPerDay>0</moistureConsumptionPerDay>
				<minMoisture>0</minMoisture>
				<minOptimalMoisture>0.5</minOptimalMoisture>
				<maxMoisture>0.9</maxMoisture>
				<minAliveTemperature>5</minAliveTemperature>
				<minLeafTemperature>5</minLeafTemperature>
				<minGrowthTemperature>8</minGrowthTemperature>
				<minGoodGrowthTemperature>14</minGoodGrowthTemperature>
				<minSunscaldTemperature>41</minSunscaldTemperature>
				<maxTemperature>41</maxTemperature>
				<ripeTemperatureThreshold>14</ripeTemperatureThreshold>
				<temperatureSensitivity>0.15</temperatureSensitivity>
				<suboptimalLightGrowthSensitivity>0</suboptimalLightGrowthSensitivity>
				<isGreenManure>false</isGreenManure>
				<growthSteps>
					<li>
						<state>Young</state>
						<max>0.65</max>
					</li>
					<li>
						<state>Ripe</state>
						<max>1</max>
					</li>
				</growthSteps>
			</li>
		</modExtensions>
	</ThingDef>

