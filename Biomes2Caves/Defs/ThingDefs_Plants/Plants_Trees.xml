<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef Name="CavePlantTree" ParentName="PlantBase" Abstract="True">
    <plant>
      <neverBlightable>true</neverBlightable>
      <growMinGlow>0</growMinGlow>
      <growOptimalGlow>0</growOptimalGlow>
      <cavePlant>true</cavePlant>
      <wildOrder>1</wildOrder>
      <wildEqualLocalDistribution>false</wildEqualLocalDistribution>
    </plant>
  </ThingDef>

<ThingDef ParentName="CavePlantTree">
		<defName>Biomes_GoldenSporecap</defName>
		<label>golden sporeCap</label>
		<description>An enormous mushroom whos fibre is strong enough to be used in place of wood for construction.</description>
		<graphicData>
			<texPath>Plants/Cave/MushroomTree/ImperialYellow</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.2, 0.3, 0.13)</volume>
				<offset>(-0.03,0,-0.44)</offset>
			</shadowData>		
		</graphicData>
		<statBases>
			<MaxHitPoints>280</MaxHitPoints>
			<Beauty>2</Beauty>
			<Flammability>0.6</Flammability>
			<Nutrition>1.6</Nutrition>
		</statBases>
		<plant>
			<fertilityMin>0</fertilityMin>
			<fertilitySensitivity>0.5</fertilitySensitivity>
			<growDays>30</growDays>
			<harvestYield>44</harvestYield>
			<harvestedThingDef>WoodLog</harvestedThingDef>
			<harvestWork>1000</harvestWork>
   			<visualSizeRange>
                		<min>1.5</min>
                		<max>2</max>
			</visualSizeRange>
			<dieIfLeafless>true</dieIfLeafless>
		</plant>
		<modExtensions>
			<li Class="Terra.ModExt_Plant">
				<plantSize>Large</plantSize>
				<plantWasteYield>9</plantWasteYield>
				<fertilityConsumptionPerDay>0.004</fertilityConsumptionPerDay>
				<moistureConsumptionPerDay>0.05</moistureConsumptionPerDay>
				<minMoisture>0.</minMoisture>
				<minOptimalMoisture>0.45</minOptimalMoisture>
				<maxMoisture>1</maxMoisture>
				<minAliveTemperature>-18</minAliveTemperature>
				<minLeafTemperature>-2</minLeafTemperature>
				<minGrowthTemperature>5</minGrowthTemperature>
				<minGoodGrowthTemperature>12</minGoodGrowthTemperature>
				<minSunscaldTemperature>50</minSunscaldTemperature>
				<maxTemperature>58</maxTemperature>
				<temperatureSensitivity>0</temperatureSensitivity>
				<isGreenManure>false</isGreenManure>
				<manureFertilityFactor>0</manureFertilityFactor>
				<growthSteps>
					<li>
						<state>Young</state>
						<max>0.4</max>
					</li>
					<li>
						<state>Ripe</state>
						<max>1</max>
					</li>
				</growthSteps>
			</li>
		</modExtensions>
	</ThingDef>
